{
  "manifest_version": 3,
  "name": "Last Tab",
  "version": "1.0",
  "description": "Quickly switch to your last used tab with a keyboard shortcut (Default: CMD/CTRL + Period)\n[chrome://extensions/shortcuts]",
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "switch-to-last-tab": {
      "suggested_key": {
        "default": "Ctrl+Period",
        "mac": "Command+Period"
      },
      "description": "Switch to last used tab"
    }
  },
  "action": {
    "default_title": "Last Tab",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "webstore-assets/icon-16.png",
    "48": "webstore-assets/icon-48.png",
    "128": "webstore-assets/icon-128.png"
  }
}
